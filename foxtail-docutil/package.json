{
  "name": "foxtail-docutil",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "esbuild --outfile=dist/foxtailcss-docutil.js --target=chrome83 --format=iife --bundle --minify src/index.js"
  },
  "author": "no_name",
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true,
      "jest": true,
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ]
  },
  "devDependencies": {
    "esbuild": "^0.14.23",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "foxtailcss": "^0.3.1",
    "prettier": "^2.4.0"
  }
}
